   "source": [
    "#prime or not\n",
    "num=int(input(\"enter a number:\"))\n",
    "count=0\n",
    "for i in range(1,round(num/2)+1):\n",
    "    if num%i==0:\n",
    "        count+=1\n",
    "if count>1:\n",
    "    print(f\"{num} is not a prime!\")\n",
    "elif count==1:\n",
    "    print(f\"{num} is a prime!\")\n"
   ]
